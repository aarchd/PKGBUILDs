pkgname=aarchd-keyring
pkgver=1.0
pkgrel=1
pkgdesc='AArchD Keyring'
arch=('any')
url='http://aarchd.github.io'
license=('GPL-3.0-or-later')
groups=('base-devel')
install="${pkgname}.install"
depends=('pacman')
source=('aarchd.gpg')
sha256sums=('edc900ce6e0a2ab6ad4bc763418704b2b774cb0dbfc5052f1869919a4a2e0fe9')

package() {
	install -dm755 "${pkgdir}"/usr/share/pacman/keyrings/
	install -m0644 aarchd.gpg "${pkgdir}"/usr/share/pacman/keyrings/
}
